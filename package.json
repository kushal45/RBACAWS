{
  "name": "rbacaws",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:rbac-core": "nest build rbac-core",
    "build:api-gateway": "nest build api-gateway",
    "build:audit-log-service": "nest build audit-log-service",
    "build:auth-service": "nest build auth-service",
    "build": "npm run build:rbac-core && npm run build:api-gateway && npm run build:audit-log-service && npm run build:auth-service",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\" \"*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"apps/**/*.ts\" \"libs/**/*.ts\" \"*.{js,ts,json,md}\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "env:setup": "./setup-env.sh",
    "env:check": "node scripts/check-env.js",
    "env:validate": "node scripts/validate-env.js",
    "migration:generate": "typeorm-ts-node-commonjs -d typeorm.config.ts migration:generate",
    "migration:run": "typeorm-ts-node-commonjs -d typeorm.config.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d typeorm.config.ts migration:revert",
    "migration:show": "typeorm-ts-node-commonjs -d typeorm.config.ts migration:show",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/rbacaws/test/jest-e2e.json",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "validate": "npm run lint:check && npm run type-check && npm run test"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^8.0.0",
    "@nestjs/throttler": "^6.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.35.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cls-hooked": "^4.3.9",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-n": "^17.23.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  }
}
